{
    "text_model_name_or_path": "FacebookAI/xlm-roberta-base",
    "output_dir": "outputs/norm/text_only_2_xml_roberta",
    "use_opensmile_features": false,
    "overwrite_output_dir": true,

    "save_total_limit": 1,
    "metric_for_classification": "accuracy",
    "metric_for_regression": "mae",
    "save_strategy": "epoch",
    "evaluation_strategy": "epoch",

    "num_labels": 2,
    "data_seed": 19,
    "data_num_folds": 1,
    "do_regression": false,
    "do_train": true,
    "do_eval": true,

    "text_max_length": 256,
    "audio_max_length": 16,
    "text_pooler_type": "max",
    "audio_pooler_type": "max",
    "per_device_train_batch_size": 1,
    "per_device_eval_batch_size": 1,
    "learning_rate": 2e-5,
    "num_train_epochs": 10,
    "save_safetensors": false
}
